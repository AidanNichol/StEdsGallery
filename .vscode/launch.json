  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "moveFiles",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/prisma/moveFiles.mjs"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "seedP",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/prisma/seedP.mjs"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "server",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/server/loader.js",
      "cwd": "${workspaceFolder}/server",
      "envFile": "${workspaceFolder}/server/.env"
    }
  ]
}
