{
  "name": "steds-gallery",
  "version": "1.0.8",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "reset": "rm gallery.sqlite&&yarn prisma migrate dev --name init",
    "seedA": "clear&&node prisma/seedA.mjs ",
    "seedP": "clear&&node prisma/seedP.mjs ",
    "moveFiles": "clear&&node prisma/moveFiles.mjs --trace-warnings ",
    "generate": "npx prisma generate",
    "server": "clear&&yarn nodemon -r dotenv/config ./server/index.js",
    "deploy": "clear && yarn version --patch && node -r dotenv/config deploy.mjs"
  },
  "dependencies": {
    "basic-ftp": "^4.6.6",
    "date-fns": "^2.21.3",
    "dotenv": "^9.0.2",
    "fastify": "^3.15.1",
    "fastify-cookie": "^5.3.1",
    "fastify-cors": "^6.0.1",
    "fastify-file-upload": "^3.0.0",
    "fastify-multipart": "^4.0.5",
    "fastify-static": "^4.0.1",
    "form-data": "^4.0.0",
    "fs-jetpack": "^4.1.0",
    "getenv": "^1.0.0",
    "jimp": "^0.16.1",
    "jspdf": "^2.3.1",
    "lodash": "^4.17.21",
    "log-update": "^4.0.0",
    "mailgun.js": "^3.3.2",
    "ospoint": "^0.2.1",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^15.0.1",
    "csv-parse": "^4.15.4",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "volta": {
    "node": "12.22.1"
  }
}
