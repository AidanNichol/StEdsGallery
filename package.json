{
  "name": "steds-gallery",
  "version": "1.0.34",
  "main": "index.js",
  "license": "MIT",
  "nodemonConfig": {
    "verbose": true,
    "ext": [
      "js",
      "mjs"
    ],
    "ignore": [
      "server/data/*.json"
    ],
    "delay": 2500
  },
  "scripts": {
    "reset": "rm gallery.sqlite&&yarn prisma migrate dev --name init",
    "seedA": "clear&&node prisma/seedA.mjs ",
    "seedP": "clear&&node prisma/seedP.mjs ",
    "moveFiles": "clear&&node prisma/moveFiles.mjs --trace-warnings ",
    "generate": "npx prisma generate",
    "server": "clear&&pnpm nodemon -r dotenv/config ./server/index.js",
    "setPrerelease": " pnpm version prerelease -f ",
    "deploy": "clear&& pnpm version patch && node -r dotenv/config ./deploy.mjs",
    "uploadGalleryDB": "clear&& node -r dotenv/config ./uploadGalleryDB.mjs",
    "downloadGalleryDB": "clear&& node -r dotenv/config ./downloadGalleryDB.mjs"
  },
  "dependencies": {
    "basic-ftp": "^4.6.6",
    "colors": "^1.4.0",
    "date-fns": "^2.21.3",
    "dotenv": "^9.0.2",
    "fastify": "^3.15.1",
    "fastify-cookie": "^5.3.1",
    "fastify-cors": "^6.0.1",
    "fastify-file-upload": "^3.0.0",
    "fastify-multipart": "^4.0.5",
    "fastify-static": "^4.0.1",
    "form-data": "^4.0.0",
    "fs-jetpack": "^4.1.0",
    "getenv": "^1.0.0",
    "jimp": "^0.16.1",
    "jspdf": "^2.3.1",
    "lodash": "^4.17.21",
    "log-update": "^4.0.0",
    "mailgun.js": "^3.3.2",
    "mysql2": "^2.3.3",
    "ospoint": "^0.2.1",
    "pino": "^8.6.1",
    "prettier": "^2.7.1",
    "sequelize": "^6.6.2",
    "textmagic-rest-client": "^1.0.11",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^15.0.1",
    "csv": "^6.2.1",
    "csv-parse": "^4.16.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "node-fetch": "^3.2.4",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "volta": {
    "node": "16.17.0"
  }
}
